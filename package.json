{
  "name": "@nemtus/symbol-sdk-openapi-generator-typescript-fetch",
  "version": "0.1.0",
  "description": "Symbol SDK for TypeScript with OpenAPI Generator typescript-fetch",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "openapi:set:version": "npx @openapitools/openapi-generator-cli version-manager set 6.0.0",
    "openapi:generate": "npx @openapitools/openapi-generator-cli generate -i ./symbol-openapi/_build/openapi3.yml -g typescript-fetch -o ./src/api -c ./openapi-generator-config.yml",
    "build": "npm run build:cjs && npm run build:esm && npm run build:cdn && node bundle",
    "build:cjs": "node pre-build-cjs && tsc --build --clean && tsc --target es5 --module commonjs && node post-build-cjs",
    "build:esm": "node pre-build-esm && tsc --build --clean && tsc --target esnext --module esnext && node post-build-esm",
    "build:cdn": "node pre-build-cdn && tsc --build --clean && tsc --target esnext --module esnext && webpack && node post-build-cdn",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemtus/symbol-sdk-openapi-generator-typescript-fetch.git"
  },
  "keywords": [
    "blockchain",
    "symbol",
    "sdk",
    "typescript"
  ],
  "author": "nemtus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nemtus/symbol-sdk-openapi-generator-typescript-fetch/issues"
  },
  "homepage": "https://github.com/nemtus/symbol-sdk-openapi-generator-typescript-fetch#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/jest": "^28.1.6",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "node-fetch": "^2.6.7",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
